<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="640" prefWidth="960" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SettingsController">
    <children>

        <Hyperlink layoutX="40.0" layoutY="32.0" onAction="#changeScreenMenuPressed" text="&lt; Menu" />

        <Label layoutX="416.0" layoutY="37.0" text="Configurações" />

        <VBox alignment="CENTER" layoutX="230.0" layoutY="225.0" spacing="50.0" GridPane.rowIndex="3">
            <children>
                <Button mnemonicParsing="false" prefHeight="70.0" prefWidth="500.0" text="Criar" textAlignment="CENTER" wrapText="true" />
                <Button mnemonicParsing="false" prefHeight="70.0" prefWidth="500.0" text="Configurações Existentes" textAlignment="CENTER" wrapText="true" />
            </children>
            <GridPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </GridPane.margin>
        </VBox>

    </children>
</AnchorPane>
